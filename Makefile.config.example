# Makefile.config
# Create a new file called Makefile.config using this file as a template for your local config

CREATE_I18N_PLACEHOLDERS := true
FETCH_INTEGRATIONS := false
GITHUB_TOKEN := ${github_personal_token}
RUN_SERVER := true
RUN_GULP := true
#DOGWEB := "LOCAL_PATH_TO_DOGWEB"
#INTEGRATIONS_CORE := "LOCAL_PATH_TO_INTEGRATIONS_CORE"

LOCALBIN := local/bin
LOCALETC := local/etc
EXEDIR := /usr/local/bin
EXE_LIST := `find gitlab/bin -type f -exec sh -c "echo ${EXEDIR}/{} | sed s@${LOCALBIN}/@@" \;`
VIRENV=hugpython
